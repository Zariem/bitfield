{
  "name": "custom-bitfield",
  "version": "1.0.0",
  "description": "A customisable bitfield that can be used for building e.g. your own permissions system (or similar). Works with any amount of bits.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "npx eslint -c .eslintrc.js --ext .ts .\\src\\",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "bitfield",
    "buffer",
    "permissions",
    "custom",
    "customisable",
    "customizable",
    "discord",
    "permission",
    "bitfields",
    "bit",
    "field",
    "unlimited",
    "size",
    "bigint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zariem/bitfield.git"
  },
  "author": "Zariem",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/Zariem/deep-profanity-filter/issues"
  },
  "homepage": "https://github.com/Zariem/deep-profanity-filter#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  },
  "files": [
    "dist/**/*"
  ]
}
